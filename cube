/*Написать функцию, которая принимает один целочисленный параметр и возвращает значение, равное этому параметру, возведенному в куб.*/
package _1_procedural;

import java.util.Scanner;


public class Cube {
    public static void main(String[] args){
        Scanner scanner = new Scanner(System.in);
        int n1 = scanner.nextInt();
        int n2 =n1*n1*n1;
        System.out.print(n2);
    }
}
